{
    "name": "google/grpc-gcp",
    "description": "gRPC GCP library for channel management",
    "license": "Apache-2.0",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/DREEBIT/grpc-php"
        }
    ],
    "require": {
        "php": "^8.0",
        "google/protobuf": "^v3.25.3||^4.26.1",
        "grpc/grpc": "dev-master",
        "google/auth": "^1.3",
        "psr/cache": "^1.0.1||^2.0.0||^3.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Grpc\\Gcp\\": "src/"
        },
        "classmap": [
            "src/generated/"
        ]
    }
}
